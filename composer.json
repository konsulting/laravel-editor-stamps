{
    "name": "konsulting/laravel-editor-stamps",
    "description": "Simple package to add creator and editor to models, and populate them during creation or update.",
    "authors": [
        {
            "name": "Keoghan Litchfield",
            "email": "keoghan@klever.co.uk"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Konsulting\\Laravel\\EditorStamps\\": "src/"
        }
    },
    "extra": {
        "laravel" : {
            "providers": [
                "Konsulting\\Laravel\\EditorStamps\\ServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "classmap": ["tests"]
    }
}
